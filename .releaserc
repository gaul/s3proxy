{
    "tagFormat": 's3proxy-${version}',
        "branches": [
            {
                "name": 'master',
                prerelease: false
            },
            {
                "name": 'releases\/+([0-9])?(\.\d+)(\.\d+|z|$)',
                prerelease: false
            },
            {
                "name": 'next',
                prerelease: false
            },
            {
                name: 'next-major',
                prerelease: true
            },
            {
                name: 'develop',
                prerelease: true
            },
            {
                name: 'develop\/.*',
                prerelease: true
            }
        ]
}
